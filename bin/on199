#!/usr/bin/env node
// rsync -avz e/dist/ bl:/home/bl_deploy/vue_lesson
var process = require('child_process');
var staticPath = __dirname.replace('bin', 'dist/');
var order = 'rsync -avz '+ staticPath +' bl:/home/bl_deploy/frontend/bl_deploy/vue_lesson';
process.exec(order,
  function (error, stdout, stderr) {
    if (error !== null) {
      console.log('exec error: ' + error);
    }
});

